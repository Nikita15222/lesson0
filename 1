#Задача 1 (просто) "Арифметика":
print("1st program")
print(9**0.5*5)

#Задача 2 (просто) "Логика":
print("2nd program")
print(9.99>9.98 and 1000!=1000.1)

#Задача 3 (средне) "Школьная загадка":
print("3rd program")
print((2*2+2)==(2*(2+2)))

#Задача 4 (сложно) "Первый после точки":
print("4th program")
print(int(float('123.456')*10%10))
